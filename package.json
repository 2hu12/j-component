{
  "name": "j-component",
  "version": "1.0.0",
  "description": "miniprogram custom component framework",
  "main": "index.js",
  "scripts": {
    "test": "jest ./test/*.test.js --silent --bail",
    "coverage": "jest ./test/*.test.js --coverage --bail",
    "coveralls": "jest ./test/*.test.js --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint \"src/**/*.js\" --fix"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testURL": "https://jest.test",
    "collectCoverageFrom": [
      "src/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JuneAndGreen/j-component.git"
  },
  "author": "june_01",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0",
    "jsdom": "^11.12.0"
  },
  "dependencies": {
    "expr-parser": "^1.0.0",
    "miniprogram-exparser": "latest"
  }
